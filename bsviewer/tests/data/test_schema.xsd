										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EnergyResource" type="auc:FuelTypes">
		<xs:annotation>
			<xs:documentation>Type of energy resource fuel. This can be applied at the premises or individual system or equipment level.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ResourceUnits">
