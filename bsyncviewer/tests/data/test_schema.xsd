								<xs:annotation>
									<xs:documentation>If "Custom" is used as the Floor Area Type, this term can be used to name and identify the custom floor area.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="FloorAreaValue" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The floor area numeric value. (ft2)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:decimal">
